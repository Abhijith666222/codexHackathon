#!/usr/bin/env python3

import runpy
from pathlib import Path


def main() -> int:
    script = Path(__file__).resolve().parent / "tools" / "codex-multi" / "orchestrator.py"
    if not script.exists():
        raise SystemExit(f"Missing orchestrator module at {script}")
    return runpy.run_path(str(script))["main"]()


if __name__ == "__main__":
    raise SystemExit(main())
